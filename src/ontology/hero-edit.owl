Prefix(:=<http://purl.obolibrary.org/obo/hero.owl#>)
Prefix(dce:=<http://purl.org/dc/elements/1.1/>)
Prefix(owl:=<http://www.w3.org/2002/07/owl#>)
Prefix(rdf:=<http://www.w3.org/1999/02/22-rdf-syntax-ns#>)
Prefix(xml:=<http://www.w3.org/XML/1998/namespace>)
Prefix(xsd:=<http://www.w3.org/2001/XMLSchema#>)
Prefix(rdfs:=<http://www.w3.org/2000/01/rdf-schema#>)
Prefix(dcterms:=<http://purl.org/dc/terms/>)


Ontology(<http://purl.obolibrary.org/obo/hero.owl>
Import(<http://purl.obolibrary.org/obo/hero/imports/bfo_import.owl>)
Import(<http://purl.obolibrary.org/obo/hero/imports/omrse_import.owl>)
Import(<http://purl.obolibrary.org/obo/hero/imports/ro_import.owl>)
Annotation(dcterms:description "The Health-Related Exposure Ontology (HERO) is an application ontology designed to facilitate the integration of data on exposures. This ontology is being developed as part of my dissertation project and is motivated by the needs of environmental epidemiologists studying the relationship between the exposome--the sum of all enviornmental factors to which a person is exposed at any point between conception and death--and both COVID-19 and hypertensive disorders of pregnancy (HDP)."@en)
Annotation(dcterms:license <https://creativecommons.org/licenses/unspecified>)
Annotation(dcterms:title "Health-Related Exposure Ontology")

Declaration(Class(<http://purl.obolibrary.org/obo/HERO_0000000>))
Declaration(Class(<http://purl.obolibrary.org/obo/HERO_00000004>))
Declaration(Class(<http://purl.obolibrary.org/obo/HERO_00000005>))
Declaration(Class(<http://purl.obolibrary.org/obo/HERO_00000006>))
Declaration(AnnotationProperty(dcterms:description))
Declaration(AnnotationProperty(dcterms:license))
Declaration(AnnotationProperty(dcterms:title))
############################
#   Annotation Properties
############################

# Annotation Property: dcterms:description (description)

AnnotationAssertion(rdfs:label dcterms:description "description")

# Annotation Property: dcterms:license (license)

AnnotationAssertion(rdfs:label dcterms:license "license")

# Annotation Property: dcterms:title (title)

AnnotationAssertion(rdfs:label dcterms:title "title")



############################
#   Classes
############################

# Class: <http://purl.obolibrary.org/obo/HERO_0000000> (environmental exposure process)

AnnotationAssertion(<http://purl.obolibrary.org/obo/IAO_0000115> <http://purl.obolibrary.org/obo/HERO_0000000> "A process p that is causally upstream of some response to an external stimulus that occurs in some organism by virtue of that organism participating in p."@en)
AnnotationAssertion(rdfs:label <http://purl.obolibrary.org/obo/HERO_0000000> "environmental exposure process"@en)
SubClassOf(<http://purl.obolibrary.org/obo/HERO_0000000> <http://purl.obolibrary.org/obo/BFO_0000015>)

# Class: <http://purl.obolibrary.org/obo/HERO_00000004> (environmental exposure process via physical contact)

AnnotationAssertion(<http://purl.obolibrary.org/obo/IAO_0000115> <http://purl.obolibrary.org/obo/HERO_00000004> "An environmental exposure process that happens via some physical contacting between an organism and a material entity."@en)
AnnotationAssertion(rdfs:label <http://purl.obolibrary.org/obo/HERO_00000004> "environmental exposure process via physical contact"@en)
SubClassOf(<http://purl.obolibrary.org/obo/HERO_00000004> <http://purl.obolibrary.org/obo/HERO_0000000>)

# Class: <http://purl.obolibrary.org/obo/HERO_00000005> (environmental exposure process via contact with external environmental process)

AnnotationAssertion(<http://purl.obolibrary.org/obo/IAO_0000115> <http://purl.obolibrary.org/obo/HERO_00000005> "An environmental exposure process that happens via some physical contacting that is a temporal part of some process the material entity is participating in."@en)
AnnotationAssertion(rdfs:label <http://purl.obolibrary.org/obo/HERO_00000005> "environmental exposure process via contact with external environmental process"@en)
SubClassOf(<http://purl.obolibrary.org/obo/HERO_00000005> <http://purl.obolibrary.org/obo/HERO_00000004>)

# Class: <http://purl.obolibrary.org/obo/HERO_00000006> (physical contacting with organism)

AnnotationAssertion(<http://purl.obolibrary.org/obo/IAO_0000115> <http://purl.obolibrary.org/obo/HERO_00000006> "A process in which some material entity overlaps with or is adjacent to an organism that it was previously separated from."@en)
AnnotationAssertion(rdfs:label <http://purl.obolibrary.org/obo/HERO_00000006> "physical contacting with organism"@en)
SubClassOf(<http://purl.obolibrary.org/obo/HERO_00000006> <http://purl.obolibrary.org/obo/BFO_0000015>)


)